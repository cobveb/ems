				<textElement verticalAlignment="Middle" markup="styled">
					<font fontName="PDF_fonts"/>
				</textElement>
                <textFieldExpression>
                    <![CDATA["20. Tryb udzielenia zamówienia: <style isBold='true'>" + $F{ORD_PROC} + "</style>"]]></textFieldExpression>
			</textField>
