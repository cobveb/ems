    <subDataset name="pzp" uuid="3b444f11-39a3-4c52-8c87-14e605e0942d">
        <parameter name="PLAN_ID" class="java.math.BigDecimal">
            <defaultValueExpression><![CDATA[new BigDecimal(142)]]></defaultValueExpression>
        </parameter>
        <queryString language="SQL">
            <![CDATA[select
	ps.id,
	(select name from emsarch.dictionary_items dict where dict.dictionary_code = 'slAsortGr' and dict.id = ppp.assortment_id) as assortment_group,
	am_req_net,
	order_type,
	DECODE(order_type, 
		'DST', 'Dostawa',
		 'USL', 'Usługa' 	
	) as order_type_name,
	(select name from emsarch.dictionary_items dict where dict.dictionary_code = 'slTryUdzZp' and dict.id = ppp.mode_id) as mode_pzp,
	DECODE(estimation_type, 
		'DO50', 'Do 50 000 zl netto',  
		'D0130', 'Powyżej 50 000 do 130 000 zl netto',
		'PO130', 'Powyżej 130 000 zł netto BZP',
		'UE139', 'Powyżej 139 000 euro netto UZP',
		'WR', 'Wolna ręka',
		'COVID', 'COVID'
	) as estimation_type_name
	
from
	cor_plan_positions ps left join cor_pub_procurement_positions ppp on (ps.id = ppp.id)
 where 
 	ps.plan_id =  $P{PLAN_ID}]]>
        </queryString>
