    <queryString language="SQL">
        <![CDATA[select
    so.type_id,
    sum(so.so_awa_gross),
    sum(so.so_rea_gross),
    sum(so.so_exp_awa_gross)
from
	emsadm.cor_plan_positions ps left join emsadm.cor_inv_founding_source so on (ps.id = so.position_id)
 where 
 	ps.plan_id =  $P{PLAN_ID} 
group by so.type_id]]>
    </queryString>
