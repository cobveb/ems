        <property name="com.jaspersoft.studio.data.sql.tables" value=""/>
        <parameter name="PLAN_ID" class="java.math.BigDecimal"/>
        <queryString language="SQL">
            <![CDATA[select
	ps.id,
	am_req_gross,
	am_awa_gross,
	cost_type_id,
	cost_number,
	name
from
	emsadm.acc_institution_plan_positions ps left join emsadm.acc_institution_plan_pos_fin fp on (ps.id = fp.id)
		left join emsadm.acc_costs_type ct on (fp.cost_type_id = ct.id)
where 
	ps.plan_id = $P{PLAN_ID}]]>
        </queryString>
