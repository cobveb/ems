    <queryString>
        <![CDATA[SELECT
	pl.id,
	pl.plan_type,
	usr.name || ' ' || usr.surname as coordinator_user,
	co.name as coordinator_name,
	pl.year
 FROM
 	emsarch.users usr left join cor_plans pl on (pl.send_user_id = usr.id)
 	left join organization_units co on (pl.coordinator_id = co.code)
WHERE
	 pl.id= $P{MAIN_ID}]]>
    </queryString>
